        /* W3C, IE10+, FF16+, Chrome26+, Opera12+, Safari7+ */
    }
    
    #pricing > input:checked + label {
        color: #fff;
        border-bottom: 0;
        background: #241E1A;
        margin-bottom: -3px;
        --webkit-box-shadow: inset 0px 1px 0px 0.5px #ddd;
        -moz-box-shadow: inset 0px 1px 0px 0.5px #ddd;
        box-shadow: inset 0px 1px 0px 0.5px #ddd;
        text-shadow: 0 -1px 0 rgba(0, 0, 0, 0.3);
    }
    
    #tab1:checked ~ #content1,
    #tab2:checked ~ #content2,
    #tab3:checked ~ #content3 {
        display: flex;
        background: rgba(255, 255, 255, 0.9);
    }
    /* table
--------------------------------------------- */
    
    .pricing-container {
        display: flex: flex-direction: column;
        width: 100%;
    }
    
    table {
        width: 100%;
        border-collapse: collapse;
    }
    
    caption {
        @extend .general-text;
        text-align: center;
        padding-bottom: 0;
        margin-bottom: 1em;
    }
    
    table > caption > h5 {
