    }
    
    input {
        display: none;
        margin: 0
    }
    
    #pricing > label {
        display: inline-block;
        width: 33.33%;
        width: calc(100% / 3);
        margin-right: -4.5px;
        padding: 15px 5px;
        font-weight: 400;
        font-family: 'Droid Serif', Serif;
        font-size: 1em;
        text-align: center;
        color: #999;
        border-bottom: 0;
        background: #4F463C;
        -webkit-box-shadow: inset 0px 1px 0px 0.5px #ddd;
        -moz-box-shadow: inset 0px 1px 0px 0.5px #ddd;
        box-shadow: inset 0px 1px 0px 0.5px #ddd;
    }
    
    #pricing > label:hover {
        color: #bbb;
        cursor: pointer;
        background: -moz-radial-gradient(center, ellipse cover, #4F463C 0%, #332A25 100%, #7db9e8 100%);
        /* FF3.6-15 */
        background: -webkit-radial-gradient(center, ellipse cover, #4F463C 0%, #332A25 100%, #7db9e8 100%);
        /* Chrome10-25,Safari5.1-6 */
