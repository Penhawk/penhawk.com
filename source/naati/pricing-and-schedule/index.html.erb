        margin-top: 0.5em;
        margin-bottom: 0em
    }
    
    table > caption > h6 {
        @extend .headings;
        font-size: 1.3em;
        line-height: 100%;
        margin-top: 1em;
        margin-bottom: 1em
    }
    
    tr {
        width: 100%;
        height: 2.3em
    }
    
    tr > td {
        @extend .general-text;
        color: #6f6f6f;
        font-weight:400;
